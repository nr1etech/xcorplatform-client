{
  "name": "@xcorplatform/client",
  "description": "Client for the Xcor Platform API",
  "version": "0.0.2-alpha.8",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "NR1E, Inc",
  "publishConfig": {
    "access": "public"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/nr1etech/xcorplatform-client-js.git"
  },
  "files": [
    "/dist/**/*.d.ts",
    "/dist/**/*.js",
    "/dist/**/*.js.map",
    "!/dist/**/*.test.*"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "prettier --check . && gts lint",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "gts lint",
    "clean": "find src \\( -name '*.js' -o -name '*.d.ts' -o -name '*.map' \\) -delete",
    "fix": "gts fix",
    "makepretty": "prettier --write .",
    "site": "typedoc --out site src/index.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.1",
    "@nr1e/commons": "0.0.3-alpha.6",
    "@nr1e/logging": "^0.0.4",
    "axios": "^1.6.3",
    "js-yaml": "^4.1.0",
    "url": "^0.11.3",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@nr1e/commons": "^0.0.3-alpha.6",
    "@nr1e/logging": "^0.0.4",
    "axios": "^1.6.3",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.1",
    "gts": "^5.2.0",
    "jest": "^29.7.0",
    "jest-runner-groups": "^2.2.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.6",
    "typedoc-plugin-zod": "^1.1.2",
    "typescript": "~5.1.6"
  }
}
